<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="rootAnalysis" prefHeight="400.0" prefWidth="600.0" stylesheets="@../styles/analysis.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainGUI.AnalysisController">
    <VBox spacing="10">
        <Pane fx:id="chartPane" />
        <HBox spacing="10">
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <Label id="labelBot" alignment="CENTER" text="Ajouter/Supprimer une courbe en fonction des types : " />
            <ComboBox fx:id="comboBox" />
            <Button onAction="#add" text=" + " />
            <Button onAction="#remove" text=" - " />
        </HBox>
    </VBox>
</AnchorPane>
